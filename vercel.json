{
  "version": 2,
  "functions": {
    "api/analyze.py": {
      "runtime": "python3.9"
    },
    "api/analyze-competitors.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/analyze-competitors",
      "dest": "/api/analyze-competitors.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}