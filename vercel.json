{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/analyze-competitors.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/analyze-competitors",
      "dest": "/api/analyze-competitors.py"
    },
    {
      "src": "/api/regions",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}