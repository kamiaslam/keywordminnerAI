{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "app.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/analyze-competitors",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/regions",
      "dest": "/api/main.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/app.js",
      "dest": "/app.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}